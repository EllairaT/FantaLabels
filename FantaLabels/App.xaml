<Application x:Class="FantaLabels.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FantaLabels"
             xmlns:viewModel="clr-namespace:FantaLabels.MVVM.ViewModel"
             xmlns:view="clr-namespace:FantaLabels.MVVM.View"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type viewModel:LabelViewModel}">
                <view:LabelView/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
